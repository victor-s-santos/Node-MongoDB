{{> header}}
<h1>Faça já o seu Cadastro:</h1>

<form method="POST">
    <label>
        <h2>Usuário:</h2>
        <input type="email" name="email" />
    </label>
    <br />
    <label>
        <h2>Senha:</h2>
        <input type="password" name="password" />
    </label>
    <br />
    <label>
        <input type="submit" value="Cadastrar" />
    </label>
</form>