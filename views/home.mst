{{> header}}
<h1>{{titulo}}</h1>
    <h1>Lista das Postagens:</h1>
{{#posts}}
    <div class="post">
        <h3><a href="/post/{{slug}}">{{title}}</a></h3>
        {{#photo}}
        <img src="/media/{{photo}}" />
        {{/photo}}
        <p>{{body}}</p>
        <a href="/post/{{slug}}/edit"> [ editar ]</a>
    </div>
{{/posts}}